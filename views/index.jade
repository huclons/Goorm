												legend(localization_key='dialog_preference_libraries_goorm_guide') goorm Core
												div(style='width:100%; height:22px;')
															div(id='core_version')(style='float:left;') Core : 
									fieldset
												legend(localization_key='dialog_preference_libraries_external_guide') External Libraries
												div(style='width:100%; height:22px;')
															div(id='yui_version')(style='float:left;') YUI Library : 
												div(style='width:100%; height:22px;')
															div(id='jquery_version')(style='float:left;') jQuery : 
												div(style='width:100%; height:22px;')
															div(id='codemirror_version')(style='float:left;') CodeMirror : 
												div(style='width:100%; height:22px;display:none')
															div(id='gcc_version')(style='float:left;') GCC : 
												div(style='width:100%; height:22px;display:none')
															div(id='gdb_version')(style='float:left;') GDB : 
												div(style='width:100%; height:22px;display:none')
															div(id='node_version_preference')(style='float:left;') Node : 
												div(style='width:100%; height:22px;display:none')
															div(id='java_version')(style='float:left;') Java : 
												div(style='width:100%; height:22px;display:none')
															div(id='python_version')(style='float:left;') Python : 
			div(id='head_project._new')
						.content_header(localization_key='dialog_new_project') New Project
						div(id='project_new')(style='text-align:left; font-size:11px;')
									form(id='project_new_import_form')(action='project/import', method='post', enctype='multipart/form-data')
												.wizard_step(step='1')
															.content_guide_message(localization_key='dialog_new_project_guide1') Select project type
															.project_types(style='float:left; width:250px; height:350px; border:1px solid #999; background-color:#fff; overflow:auto;')
															div(style='float:right; width:520px; height:350px; border:1px solid #999;')
																		.project_items(style='width:100%; height:270px; border-bottom:1px solid #999; background-color:#fff; text-align:center; overflow:auto; line-height:12px;')
																		.project_description(style='width:100%; height:79px; background-color:#FFF; overflow:auto;')
																					div(style='padding:10px;')
																								b(localization_key='dialog_new_project_description') Project description
																								br
																								div(id='text_project_description')(style='width:100%; padding:5px;')
																								input(id='input_project_type')(type='hidden', checkfield='yes')
																								input(id='input_project_plugin')(type='hidden', checkfield='yes')
																								input(id='input_project_detailed_type')(type='hidden', checkfield='yes')
												.wizard_step(step='2', style='display:none;')
															.content_guide_message(localization_key='dialog_new_project_guide2') Add information for new project
															div(style='float:left; width:770px; height:340px; overflow:auto; padding:5px;')
																		div(style='float:left; width:770px; height:200px;')
																					fieldset(style='padding:10px; padding-top:30px; border:1px solid #ccc; margin-top:5px; background-color:#FBFBFB;')
																								legend
																											b(localization_key='dialog_new_project_information') Project information
																								.project_author(style='height:25px;')
																											div(style='width:120px; float:left; height:25px;', localization_key='dialog_new_project_author') Author ID
																											div(style='width:615px; float:right; height:25px;')
																														input(id='input_project_author')(type='text', name='input_project_author', style='width:603px;')
																								.project_author_name(style='height:25px;')
																											div(style='width:120px; float:left; height:25px;', localization_key='dialog_new_project_author_name') Author Name
																											div(style='width:615px; float:right; height:25px;')
																														input(id='input_project_author_name')(type='text', name='input_project_author_name', style='width:603px;')
																								.project_name(style='height:25px;')
																											div(style='width:120px; float:left; height:25px;', localization_key='dialog_new_project_name') Project Name
																											div(style='width:615px; float:right; height:25px;')
																														input(id='input_project_name')(type='text', name='input_project_name', style='width:603px;')
																								.project_desc(style='height:45px;')
																											div(style='width:120px; float:left; height:45px;', localization_key='dialog_new_project_desc') Project Description
																											div(style='width:615px; float:right; height:45px;')
																														textarea(id='input_project_desc')(style='width:603px; height:40px;', draggable='false')
																					fieldset(id='import_field')(style='padding:10px; padding-top:30px; background-color:#FBFBFB;')
																								legend
																											b(localization_key='dialog_new_project_import') Project import
																								div(style='width:200px; height:22px; float:left;')
																											label(for='check_project_new_import')
																														div(style='float:left; width:15px; height:20px;')
																																	input(id='check_project_new_import')(type='checkbox')
																														div(style='float:left; width:160px; height:16px; margin-top:-2px;', localization_key='dialog_new_project_import_use') Import project
																								div(id='project_new_import_div')(style='display:none; padding:4px; background:#fff; width:500px; margin-top:-8px; float:right;')
																											div(style='width:220px; float:left; padding:2px; font-size:11px;', localization_key='dialog_new_project_import_message') Select zip file to import
																											div(style='width:270px; float:left; font-size:11px; text-align:right;')
																														div(style='float:left;')
																																	div(id='project_new_import_upload_output')
																																	input(id='project_new_import_file')(type='file', name='file')
																																	input(id='project_new_location')(type='hidden', name='project_import_location', readonly='readonly')
																		input(id='new_project_using_plugin')(type='hidden', value='no')
																		div(id='new_project_expansion_container')(style='width:770px;')
			div(id='head_tutorial_intro')
						div(id='tutorial-intro').tutorial-dialog(title='튜토리얼')
									img(id='tutorial-intro-img')(src='modules/org.goorm.core.tutorial/book.jpg', align='left')
									| 본 시스템은 "HTML5+jQuery" 서적의 다양한 예제를 직접 개발하고 테스트 할 수 있는 클라우드 기반의 통합 개발환경입니다. "튜토리얼 시작"을 누르면, 간단한 시스템 사용방법을 안내 받으실 수 있습니다.
			div(id='head_project._import')
						.content_header Import Project
						div(id='project_import')(style='text-align:left; font-size:11px;')
									form(id='project_import_form')(action='project/import', method='post', enctype='multipart/form-data')
												div
															.content_guide_message Input Information for Import Project
															div(style='float:left; width:770px; height:340px; overflow:auto; padding:5px;')
																		div(style='float:left; width:770px; height:200px;')
																					fieldset(style='padding:10px; padding-top:30px; border:1px solid #ccc; margin-top:5px; background-color:#FBFBFB;')
																								legend
																											b Project Information
																								.project_author(style='height:25px;')
																											div(style='width:120px; float:left; height:25px;') Author ID
																											div(style='width:615px; float:right; height:25px;')
																														input(id='input_import_project_author')(type='text', name='input_project_author', style='width:603px;')
																								.project_author_name(style='height:25px;')
																											div(style='width:120px; float:left; height:25px;') Author Name
																											div(style='width:615px; float:right; height:25px;')
																														input(id='input_import_project_author_name')(type='text', name='input_project_author_name', style='width:603px;')
																								.project_type(style='height:25px;')
																											div(style='width:120px; float:left; height:25px;') Project Type
																											div(style='width:615px; float:right; height:25px;')
																														select(id='select_import_project_type')(name='select_import_project_type', style='width:100%', project_type='cpp', project_detailed_type='c', plugin_name='org.goorm.plugin.cpp')
																											input(id='input_import_project_type')(type='hidden', checkfield='yes')
																											input(id='input_import_project_plugin')(type='hidden', checkfield='yes')
																											input(id='input_import_project_detailed_type')(type='hidden', checkfield='yes')
																								.project_name(style='height:25px;')
																											div(style='width:120px; float:left; height:25px;') Project Name
																											div(style='width:615px; float:right; height:25px;')
																														input(id='input_import_project_name')(type='text', name='input_project_name', style='width:603px;')
																								.project_desc(style='height:45px;')
																											div(style='width:120px; float:left; height:45px;') Project Description
																											div(style='width:615px; float:right; height:45px;')
																														textarea(id='input_import_project_desc')(style='width:603px; height:40px;', draggable='false')
																					fieldset(id='import_project_field')(style='border:3px dashed #aaa;padding:10px; padding-top:30px; background-color:#FBFBFB;')
																								legend
																											b Project Import
																								div(id='project_import_div')(style='padding:4px;			margin-top:3px; ')
																											div(style='width:220px; float:left; padding:2px; font-size:11px;') Drag a zip file in this box to import
																											div(style=' float:right; font-size:11px; text-align:right;')
																														div(style='float:left;')
																																	span.directory_icon.file.filetype-etc.filetype-zip
																																	div(id='project_import_upload_output')
																																	input(id='project_import_location')(type='hidden', name='project_import_location', readonly='readonly')
																		input(id='import_project_using_plugin')(type='hidden', value='no')
																		div(id='import_project_expansion_container')(style='width:770px;')
			div(id='head_nodejs.preference')
						div(id='selectedPreferenceTitle') node.js Plugin
						div(style='font-size:11px;')
									fieldset(style='padding:10px; border:1px solid #ccc; margin-top:5px;background: white;')
												div(style='margin-bottom: 5px;')
															| Default Main :
															input(type='text', name='plugin.nodejs.main')
												div(style='margin-bottom: 5px;')
															| Default Source Path :
															input(type='text', name='plugin.nodejs.source_path')
												div(style='margin-bottom: 5px;')
															| Default Debug Option :
															input(type='text', name='plugin.nodejs.debug_option')
						div(style='position: absolute;bottom: 10px;right:10px;')
									input.restore_default(type='button', value='Restore Default')
									input.apply(type='button', value='Apply')
			div(id='head_nodejs.property')
						div(id='selectedPreferenceTitle') node.js Plugin
						div(style='font-size:11px;')
									fieldset(style='padding:10px; border:1px solid #ccc; margin-top:5px;background: white;')
												div(style='margin-bottom: 5px;')
															| main :
															input(type='text', name='plugin.nodejs.main')
												div(style='margin-bottom: 5px;')
															| source path :
															input(type='text', name='plugin.nodejs.source_path')
												div(style='margin-bottom: 5px;')
															| debug option :
															input(type='text', name='plugin.nodejs.debug_option')
						div(style='position: absolute;bottom: 10px;right:10px;')
									input.restore_default(type='button', value='Restore Default')
									input.apply(type='button', value='Apply')
			div(id='head_project.explorer.folder')
						div(id='project.explorer.folder_context')
									.bd
												ul.first-of-type
															li.yuimenuitem
																		a.yuimenuitemlabel(action='new_context', localization_key='file_new_context') New
																		div(id='submenu_new').yuimenu
																					div(id='folder_open_context_div').bd
																								ul.first-of-type
																											li.yuimenuitem
																														a.yuimenuitemlabel(href='#', action='new_file_file_context', localization_key='file_new_context_new_file') File
																											li.yuimenuitem
																														a.yuimenuitemlabel(href='#', action='new_file_folder_context', localization_key='file_new_context_new_folder') Folder
																											li.yuimenuitem
																														a.yuimenuitemlabel(href='#', action='new_file_textfile_context', localization_key='file_new_context_untitled_text_file') Untitled Text File
																								ul(id='folder_open_context_ul')
												ul
															li.yuimenuitem
																		a.yuimenuitemlabel(action='Copy_context', localization_key='Copy_context') Copy
															li.yuimenuitem
																		a.yuimenuitemlabel(action='Paste_context', localization_key='Paste_context') Paste
												ul
															li.yuimenuitem
																		a.yuimenuitemlabel(action='folder_open_context', localization_key='folder_open_context') Open
															li.yuimenuitem(style='display:none;')
																		a.yuimenuitemlabel(action='folder_close_context', localization_key='folder_close_context') Close
															li.yuimenuitem
																		a.yuimenuitemlabel(action='rename_context', localization_key='file_rename_context') Rename
															li.yuimenuitem
																		a.yuimenuitemlabel(action='move_context', localization_key='file_move_context') Move
															li.yuimenuitem
																		a.yuimenuitemlabel(action='delete_context', localization_key='file_delete_context') Delete
															li.yuimenuitem
																		a.yuimenuitemlabel(action='delete_all_context', localization_key='delete_all_context') Delete All
												ul
															li.yuimenuitem
																		a.yuimenuitemlabel(action='import_file', localization_key='file_import', style='background-repeat: no-repeat') Import/Upload File
															li.yuimenuitem
																		a.yuimenuitemlabel(action='export_file', localization_key='file_export', style='background-repeat: no-repeat') Export File
												ul
															li.yuimenuitem
																		a.yuimenuitemlabel(action='property', localization_key='file_property_context') Properties
			div(id='head_tutorial_step1')
						div(id='tutorial-step1').tutorial-dialog(title='새 프로젝트 만들기')
									| 책의 예제를 테스트하기 위해 새 프로젝트를 만들어 봅시다.
									br
									| 상단 좌측에 "새 프로젝트" 버튼을 클릭하여 새 프로젝트 창을 띄웁니다.
			div(id='head_design.setting')
						div(style='font-size:11px; margin-top:10px;')
									fieldset
												legend Designer Configuration
												div(style='width:100%; height:22px;')
															.checkbox_div(style='float:left;')
																		span.checkbox
																					input(id='show_grid').checkbox_margin(type='checkbox', name='preference.designer.show_grid')
