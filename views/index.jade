extends layout

block content
	body.yui-skin-sam
		#goorm(style='display:none;')

		#goorm_user_box(style='display:none;')
			#goorm_user_image
			#goorm_user_info
				| User Name
				br
				| (email)

		#goorm_top.yui-navset.goorm_layout(style='display:none')
			include goorm/mainmenu
			#goorm_main_toolbar.main_toolbar(style='float:left;')
	
		#goorm_left.yui-navset.goorm_layout(style='display:none')
	
		#goorm_center_inner_layout.yui-navset.goorm_layout(style="display:none")
			#goorm_inner_layout_right.yui-navset
			#goorm_inner_layout_center.yui-navset
			#goorm_inner_layout_bottom.yui-navset
	
		#goorm_bottom.yui-navset.goorm_layout(style="display:none")
			div(style='float:left; width:100%')
				#goorm_statusbar_device_type
					div(style='float:left; margin-right:4px;')
						div.device-icon
					.device_type 
	
				#goorm_statusbar_goorm_version
					div(style='float:left; margin-right:4px;')
						div.version-icon
					.goorm_version
	
				#goorm_statusbar_version
					div(style='float:left; margin-right:4px;')
						div.browser-icon
					.browser_name
	
				div(style='float:right;')
					#goorm_statusbar_message
					#goorm_loading_indicator
						img(src='')
					#goorm_progress_bar
	
		#goorm_menu_container
		#goorm_dialog_container
		#goorm_template_container(style='display:none;')
			div(id='head_web.preference')
						div(id='selectedPreferenceTitle') Web Plugin
						div(style='font-size:11px;')
									fieldset(style='padding:10px; border:1px solid #ccc; margin-top:5px;background: white;')
												| Web plugin
						div(style='position: absolute;bottom: 10px;right:10px;')
									input.restore_default(type='button', value='Restore Default')
									input.apply(type='button', value='Apply')
			div(id='head_window.manager.tabview')
						div(id='window.manager.tabview_context')
									.bd
												ul.first-of-type
															li.yuimenuitem.first-of-type
