															li.yuimenuitem
																		a.yuimenuitemlabel(action='open_file', localization_key='file_open_file_context') Open File
															li.yuimenuitem
																		a.yuimenuitemlabel(action='close_all', localization_key='file_close_all_context') Close All
												ul
															li.yuimenuitem
																		a.yuimenuitemlabel(action='open_recent_file', localization_key='open_recent_file') Open Recent File
												ul
															li.yuimenuitem.first-of-type
																		a.yuimenuitemlabel(action='previous_window', localization_key='window_previous_window_context') Previous Window
															li.yuimenuitem
																		a.yuimenuitemlabel(action='next_window', localization_key='window_next_window_context') Next Window
			div(id='head_project.collaboration')
						div(style='font-size:11px;')
									div(style='margin-bottom:5px; height:25px;')
												div
															label(for='collaboration_check')
